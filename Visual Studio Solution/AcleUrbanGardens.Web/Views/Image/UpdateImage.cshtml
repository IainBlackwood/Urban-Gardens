@using AcleUrbanGardens.Web.Helpers
@model AcleUrbanGardens.Web.Models.UpdateImageViewModel

@{
    ViewBag.Title = "Update Image";
}

<div class="container-fluid" id="Index" style="padding-right: 20px">
    <br />
    <div class="jumbotron container-fluid" style="box-shadow: 10px 10px 5px 0px rgba(0,0,0,0.75)">
        <h2>@ViewBag.Title for @Model.Controller: (@Model.ObjectName)</h2>
        <div class="row">
            <div class="col-lg-6 col-md-6 col-sm-6" style="width: 100%; height: 400px; border: 1px solid gray; border-radius: 5px;">
                <h4>Original Image</h4>
            </div>
            <div class="col-lg-6 col-md-6 col-sm-6" style="width: 100%; height: 400px; border: 1px solid gray; border-radius: 5px; text-align: right">
                <h4>Updated Image</h4>
            </div>
        </div>
    </div>
</div>
